    "homepage": "https://pyncer.com/http",
    "license": "Unlicense",
    "require": {
        "php": ">=8.1",
        "psr/http-client": "^1.0.3",
        "psr/http-message": "^2.0.0",
        "psr/http-server-handler": "^1.0.2",
        "psr/http-server-middleware": "^1.0.2",
        "psr/http-factory": "^1.0.2",
        "psr/log": "^3.0.0",
        "pyncer/container": "^1.0",
        "pyncer/core": "^1.2",
        "pyncer/exception": "^1.0"
    },
    "autoload": {
